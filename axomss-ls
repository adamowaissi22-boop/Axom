local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local AxomFolder = ReplicatedStorage:WaitForChild("AxomServerside")
local Remotes = {
    CheckWhitelist = AxomFolder:WaitForChild("CheckWhitelist"),
    RequestInterface = AxomFolder:WaitForChild("RequestInterface"),
    ExecuteScript = AxomFolder:WaitForChild("ExecuteScript"),
    SpawnAsset = AxomFolder:WaitForChild("SpawnAsset"),
    InsertTool = AxomFolder:WaitForChild("InsertTool")
}

local isWhitelisted = false

Remotes.CheckWhitelist.OnClientEvent:Connect(function(whitelisted)
    isWhitelisted = whitelisted
    if whitelisted then
        createInterface()
    end
end)

Remotes.RequestInterface.OnClientEvent:Connect(function(granted)
    if granted then
        createInterface()
    end
end)

local function createInterface()
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "ScreenGui"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = playerGui

    local ToggleButton = Instance.new("TextButton")
    ToggleButton.Name = "ToggleButton"
    ToggleButton.Size = UDim2.new(0, 60, 0, 60)
    ToggleButton.Position = UDim2.new(0, 20, 0, 20)
    ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
    ToggleButton.Text = "Axom"
    ToggleButton.TextColor3 = Color3.new(1, 1, 1)
    ToggleButton.TextScaled = true
    ToggleButton.Font = Enum.Font.FredokaOne
    ToggleButton.ZIndex = 10
    ToggleButton.Parent = ScreenGui

    local ToggleCorner = Instance.new("UICorner")
    ToggleCorner.CornerRadius = UDim.new(1, 0)
    ToggleCorner.Parent = ToggleButton

    local MainFrame = Instance.new("Frame")
    MainFrame.Name = "MainFrame"
    MainFrame.Size = UDim2.new(0, 700, 0, 500)
    MainFrame.Position = UDim2.new(0.5, -350, 0.5, -250)
    MainFrame.BackgroundColor3 = Color3.fromRGB(173, 216, 230)
    MainFrame.BackgroundTransparency = 0.5
    MainFrame.Visible = false
    MainFrame.Active = true
    MainFrame.Draggable = true
    MainFrame.Parent = ScreenGui

    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 12)
    MainCorner.Parent = MainFrame

    local MainStroke = Instance.new("UIStroke")
    MainStroke.Color = Color3.fromRGB(100, 150, 200)
    MainStroke.Thickness = 2
    MainStroke.Parent = MainFrame

    local CloseButton = Instance.new("TextButton")
    CloseButton.Name = "CloseButton"
    CloseButton.Size = UDim2.new(0, 30, 0, 30)
    CloseButton.Position = UDim2.new(1, -35, 0, 10)
    CloseButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
    CloseButton.Text = "X"
    CloseButton.TextColor3 = Color3.new(1, 1, 1)
    CloseButton.Font = Enum.Font.GothamBold
    CloseButton.TextScaled = true
    CloseButton.Parent = MainFrame

    local CloseCorner = Instance.new("UICorner")
    CloseCorner.CornerRadius = UDim.new(0, 8)
    CloseCorner.Parent = CloseButton

    local SideBar = Instance.new("Frame")
    SideBar.Name = "SideBar"
    SideBar.Size = UDim2.new(0, 150, 1, -20)
    SideBar.Position = UDim2.new(0, 10, 0, 10)
    SideBar.BackgroundColor3 = Color3.fromRGB(200, 230, 255)
    SideBar.BackgroundTransparency = 0.3
    SideBar.Parent = MainFrame

    local SideBarCorner = Instance.new("UICorner")
    SideBarCorner.CornerRadius = UDim.new(0, 8)
    SideBarCorner.Parent = SideBar

    local SideBarStroke = Instance.new("UIStroke")
    SideBarStroke.Color = Color3.fromRGB(150, 180, 220)
    SideBarStroke.Thickness = 2
    SideBarStroke.Parent = SideBar

    local SideBarLayout = Instance.new("UIListLayout")
    SideBarLayout.Padding = UDim.new(0, 5)
    SideBarLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    SideBarLayout.VerticalAlignment = Enum.VerticalAlignment.Top
    SideBarLayout.Parent = SideBar

    local SideBarPadding = Instance.new("UIPadding")
    SideBarPadding.PaddingTop = UDim.new(0, 10)
    SideBarPadding.Parent = SideBar

    local ContentFrame = Instance.new("Frame")
    ContentFrame.Name = "ContentFrame"
    ContentFrame.Size = UDim2.new(1, -190, 1, -20)
    ContentFrame.Position = UDim2.new(0, 170, 0, 10)
    ContentFrame.BackgroundTransparency = 1
    ContentFrame.Parent = MainFrame

    local ContentScrolling = Instance.new("ScrollingFrame")
    ContentScrolling.Name = "ContentScrolling"
    ContentScrolling.Size = UDim2.new(1, 0, 1, 0)
    ContentScrolling.BackgroundTransparency = 1
    ContentScrolling.BorderSizePixel = 0
    ContentScrolling.ScrollBarThickness = 8
    ContentScrolling.Parent = ContentFrame

    local ContentLayout = Instance.new("UIGridLayout")
    ContentLayout.CellPadding = UDim2.new(0, 10, 0, 10)
    ContentLayout.CellSize = UDim2.new(0, 150, 0, 150)
    ContentLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    ContentLayout.SortOrder = Enum.SortOrder.LayoutOrder
    ContentLayout.Parent = ContentScrolling

    local function createSectionButton(name)
        local button = Instance.new("TextButton")
        button.Name = name .. "Button"
        button.Size = UDim2.new(0.9, 0, 0, 40)
        button.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
        button.Text = name
        button.TextColor3 = Color3.new(1, 1, 1)
        button.TextScaled = true
        button.Font = Enum.Font.FredokaOne
        button.AutoButtonColor = false
        button.Parent = SideBar
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = button
        
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.fromRGB(80, 130, 220)
        stroke.Thickness = 2
        stroke.Parent = button
        
        return button
    end

    local ScriptLibraryButton = createSectionButton("Script Library")
    local VehiclesButton = createSectionButton("Vehicles")
    local FlagsButton = createSectionButton("Flags")
    local ToolsButton = createSectionButton("Tools")
    local ExecutorButton = createSectionButton("Executor")

    local function clearContent()
        for _, child in ipairs(ContentScrolling:GetChildren()) do
            if not child:IsA("UIGridLayout") then
                child:Destroy()
            end
        end
    end

    local function createCardButton(name, callback)
        local card = Instance.new("TextButton")
        card.Name = name .. "Card"
        card.Size = UDim2.new(0, 150, 0, 150)
        card.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
        card.Text = name
        card.TextColor3 = Color3.new(1, 1, 1)
        card.TextScaled = true
        card.Font = Enum.Font.FredokaOne
        card.AutoButtonColor = false
        card.Parent = ContentScrolling
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 12)
        corner.Parent = card
        
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.fromRGB(80, 130, 220)
        stroke.Thickness = 3
        stroke.Parent = card
        
        card.MouseEnter:Connect(function()
            card.BackgroundColor3 = Color3.fromRGB(120, 170, 255)
        end)
        
        card.MouseLeave:Connect(function()
            card.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
        end)
        
        card.MouseButton1Click:Connect(callback)
        
        return card
    end

    ScriptLibraryButton.MouseButton1Click:Connect(function()
        clearContent()
        
        local scriptList = {
            {"Goner", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-FE-Goner-Scrpit-82044'))()"},
            {"Void Boss", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-FE-Void-Boss-53777'))()"},
            {"Cleetus", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Cleetus-Converted-77740'))()"},
            {"soul reaper", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Soul-Reaper-55330'))()"},
            {"vereus monster", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Roblox-VEREUS-monster-script-3746'))()"},
            {"Caducus", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-caducus-visual-CREDITS-TO-IM-PATRICK-71280'))()"},
            {"Pandora", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Pandora-Fe-55559'))()"},
            {"Void Dragon", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Void-dragon-46302'))()"},
            {"Killbot", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Killbot-Gauntlet-51822'))()"},
            {"Ban Hammer", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Ban-Hammer-Script-58232'))()"},
            {"MrByeBye", "loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-Mr-bye-bye-check-description-73940'))()"}
        }
        
        for _, scriptInfo in ipairs(scriptList) do
            local name, loadstringCmd = scriptInfo[1], scriptInfo[2]
            createCardButton(name, function()
                Remotes.ExecuteScript:FireServer(loadstringCmd)
            end)
        end
    end)

    VehiclesButton.MouseButton1Click:Connect(function()
        clearContent()
        
        local vehicleList = {
            {"Super Car", "15681295144"},
            {"f1 car", "106844475991344"},
            {"supra", "5457007929"},
            {"bmw car", "10708581052"},
            {"Lamborghini car", "5211116333"},
            {"bike", "3775846262"},
            {"Bmw moto", "9846492074"},
            {"Panzer WWe Tank", "83315905996841"}
        }
        
        for _, vehicleInfo in ipairs(vehicleList) do
            local name, id = vehicleInfo[1], vehicleInfo[2]
            createCardButton(name, function()
                Remotes.SpawnAsset:FireServer(id, "vehicle")
            end)
        end
    end)

    FlagsButton.MouseButton1Click:Connect(function()
        clearContent()
        
        local flagList = {
            {"Lebanon ðŸ‡±ðŸ‡§", "4174138"},
            {"Armenia ðŸ‡¦ðŸ‡²", "2155508886"},
            {"Philippines ðŸ‡µðŸ‡­", "13542883640"},
            {"Russia ðŸ‡·ðŸ‡º", "14586815502"},
            {"USA ðŸ‡ºðŸ‡¸", "9488198458"},
            {"United Kingdom ðŸ‡¬ðŸ‡§", "9488220262"},
            {"Sweden ðŸ‡¸ðŸ‡ª", "5144921822"},
            {"Egypt ðŸ‡ªðŸ‡¬", "5344743080"},
            {"Japan ðŸ‡¯ðŸ‡µ", "5390866768"},
            {"France ðŸ‡«ðŸ‡·", "15398781631"},
            {"Vietnam ðŸ‡»ðŸ‡³", "6979252261"},
            {"Iraq ðŸ‡®ðŸ‡¶", "5344788585"},
            {"Palestine ðŸ‡µðŸ‡¸", "83354671819470"},
            {"Germany ðŸ‡©ðŸ‡ª", "1428700073"}
        }
        
        for _, flagInfo in ipairs(flagList) do
            local name, id = flagInfo[1], flagInfo[2]
            createCardButton(name, function()
                Remotes.SpawnAsset:FireServer(id, "flag")
            end)
        end
    end)

    ToolsButton.MouseButton1Click:Connect(function()
        clearContent()
        
        local toolList = {
            {"Spartan Shield", "430249190"},
            {"Riot Shield", "4626276803"},
            {"Azure sword", "10288498712"},
            {"Devil Sword", "13078237623"},
            {"Godly Ban Hammer", "8821996850"},
            {"money tool", "5163961721"},
            {"lantern tool", "3060834159"},
            {"flashlight", "10093563208"},
            {"btools", "142785488"}
        }
        
        for _, toolInfo in ipairs(toolList) do
            local name, id = toolInfo[1], toolInfo[2]
            createCardButton(name, function()
                Remotes.InsertTool:FireServer(id)
            end)
        end
    end)

    ExecutorButton.MouseButton1Click:Connect(function()
        clearContent()
        
        local ExecutorFrame = Instance.new("Frame")
        ExecutorFrame.Name = "ExecutorFrame"
        ExecutorFrame.Size = UDim2.new(1, 0, 1, -80)
        ExecutorFrame.BackgroundTransparency = 1
        ExecutorFrame.Parent = ContentScrolling
        
        local ScriptBox = Instance.new("TextBox")
        ScriptBox.Name = "ScriptBox"
        ScriptBox.Size = UDim2.new(1, -20, 1, -10)
        ScriptBox.Position = UDim2.new(0, 10, 0, 10)
        ScriptBox.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        ScriptBox.TextColor3 = Color3.new(1, 1, 1)
        ScriptBox.Text = ""
        ScriptBox.TextScaled = false
        ScriptBox.TextSize = 14
        ScriptBox.Font = Enum.Font.Code
        ScriptBox.TextXAlignment = Enum.TextXAlignment.Left
        ScriptBox.TextYAlignment = Enum.TextYAlignment.Top
        ScriptBox.ClearTextOnFocus = false
        ScriptBox.MultiLine = true
        ScriptBox.TextWrapped = true
        ScriptBox.PlaceholderText = "Enter script here..."
        ScriptBox.Parent = ExecutorFrame
        
        local BoxCorner = Instance.new("UICorner")
        BoxCorner.CornerRadius = UDim.new(0, 8)
        BoxCorner.Parent = ScriptBox
        
        local BoxStroke = Instance.new("UIStroke")
        BoxStroke.Color = Color3.fromRGB(100, 100, 150)
        BoxStroke.Thickness = 2
        BoxStroke.Parent = ScriptBox
        
        local ButtonFrame = Instance.new("Frame")
        ButtonFrame.Name = "ButtonFrame"
        ButtonFrame.Size = UDim2.new(1, -20, 0, 60)
        ButtonFrame.Position = UDim2.new(0, 10, 1, 10)
        ButtonFrame.BackgroundTransparency = 1
        ButtonFrame.Parent = ContentScrolling
        
        local ClearButton = Instance.new("TextButton")
        ClearButton.Name = "ClearButton"
        ClearButton.Size = UDim2.new(0.45, 0, 1, 0)
        ClearButton.Position = UDim2.new(0, 0, 0, 0)
        ClearButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        ClearButton.Text = "Clear"
        ClearButton.TextColor3 = Color3.new(1, 1, 1)
        ClearButton.TextScaled = true
        ClearButton.Font = Enum.Font.FredokaOne
        ClearButton.AutoButtonColor = false
        ClearButton.Parent = ButtonFrame
        
        local ClearCorner = Instance.new("UICorner")
        ClearCorner.CornerRadius = UDim.new(0, 8)
        ClearCorner.Parent = ClearButton
        
        local ClearStroke = Instance.new("UIStroke")
        ClearStroke.Color = Color3.fromRGB(200, 60, 60)
        ClearStroke.Thickness = 2
        ClearStroke.Parent = ClearButton
        
        local ExecuteButton = Instance.new("TextButton")
        ExecuteButton.Name = "ExecuteButton"
        ExecuteButton.Size = UDim2.new(0.45, 0, 1, 0)
        ExecuteButton.Position = UDim2.new(0.55, 0, 0, 0)
        ExecuteButton.BackgroundColor3 = Color3.fromRGB(80, 180, 80)
        ExecuteButton.Text = "Execute"
        ExecuteButton.TextColor3 = Color3.new(1, 1, 1)
        ExecuteButton.TextScaled = true
        ExecuteButton.Font = Enum.Font.FredokaOne
        ExecuteButton.AutoButtonColor = false
        ExecuteButton.Parent = ButtonFrame
        
        local ExecuteCorner = Instance.new("UICorner")
        ExecuteCorner.CornerRadius = UDim.new(0, 8)
        ExecuteCorner.Parent = ExecuteButton
        
        local ExecuteStroke = Instance.new("UIStroke")
        ExecuteStroke.Color = Color3.fromRGB(60, 150, 60)
        ExecuteStroke.Thickness = 2
        ExecuteStroke.Parent = ExecuteButton
        
        ClearButton.MouseEnter:Connect(function()
            ClearButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
        end)
        
        ClearButton.MouseLeave:Connect(function()
            ClearButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        end)
        
        ExecuteButton.MouseEnter:Connect(function()
            ExecuteButton.BackgroundColor3 = Color3.fromRGB(100, 200, 100)
        end)
        
        ExecuteButton.MouseLeave:Connect(function()
            ExecuteButton.BackgroundColor3 = Color3.fromRGB(80, 180, 80)
        end)
        
        ClearButton.MouseButton1Click:Connect(function()
            ScriptBox.Text = ""
        end)
        
        ExecuteButton.MouseButton1Click:Connect(function()
            local scriptContent = ScriptBox.Text
            if scriptContent and #scriptContent > 0 then
                Remotes.ExecuteScript:FireServer(scriptContent)
            end
        end)
    end)

    ToggleButton.MouseButton1Click:Connect(function()
        MainFrame.Visible = not MainFrame.Visible
        if MainFrame.Visible then
            ScriptLibraryButton:Fire()
        end
    end)

    CloseButton.MouseButton1Click:Connect(function()
        MainFrame.Visible = false
    end)

    ScriptLibraryButton:Fire()
end

Remotes.CheckWhitelist:FireServer()

task.wait(2)

if not isWhitelisted then
    Remotes.RequestInterface:FireServer()
end
