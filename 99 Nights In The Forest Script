                    for _, item in pairs(items:GetChildren()) do
                        if item:IsA("Model") and item.PrimaryPart and (table.find(autoFuelTypes, "All") or table.find(autoFuelTypes, item.Name)) then
                            if item.Name == "Log" or item.Name == "Fuel Canister" or item.Name == "Oil Barrel" or item.Name == "Coal" then
                                pcall(function()
                                    ReplicatedStorage.RemoteEvents.RequestStartDraggingItem:FireServer(item)
                                    task.wait(0.05)
                                    item:SetPrimaryPartCFrame(CFrame.new(0, 20, 0))
                                end)
                            end
                        end
                    end
                end
                task.wait(1)
            end
        end)
    else
        if autoFuelThread then
            task.cancel(autoFuelThread)
            autoFuelThread = nil
        end
        Window:MakeNotification("Auto Fuel Fire", "Disabled", 3)
    end
end)

AutoFeatures:MakeToggle("Auto Sacrifice Cultists", false, function(enabled)
